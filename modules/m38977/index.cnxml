                          </m:msub>
                          <m:mo>·</m:mo>
                          <m:mi>m</m:mi>
                        </m:mrow>
                        <m:msubsup>
                          <m:mi>r</m:mi>
                          <m:mi>E</m:mi>
                          <m:mn>2</m:mn>
                        </m:msubsup>
                      </m:mfrac>
