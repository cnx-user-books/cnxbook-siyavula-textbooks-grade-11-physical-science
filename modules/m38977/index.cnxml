                    <m:mo>=</m:mo>
                  </m:mtd>
                  <m:mtd columnalign="left">
                    <m:mrow>
                      <m:mn>2</m:mn>
                      <m:mo>(</m:mo>
                      <m:mi>G</m:mi>
                      <m:mfrac>
                        <m:mrow>
                          <m:msub>
